[user]
	name = Ali Kaafarani
	email = kaafarani.ali@gmail.com
[alias]
        ci = commit
        st = status
        br = branch
        co = checkout
        df = diff
	lg = log --graph --abbrev-commit --date=relative --pretty=format:'%Cred%h%Creset -%C(yellow)%d%Creset %s %Cgreen(%cr)%Creset'
        # --pretty=format:'%Cred%h%Creset -%Creset %s %Cgreen(%cr)%Creset'
[branch]
	autosetupmerge = always
[push]
	default = tracking
[core]
	editor = vim
#[achievement]
#	upload = true
[color]
        branch = true
        status = true
        diff = true
[log]
        date = relative
[help]
        format = man #/html
        browser = opera
        autocorrect = -20
[instaweb]
        httpd = lighttpd
        browser = opera
[web]
        browser = opera
[browser "opera"]
        cmd = opera
