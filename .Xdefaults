! XCursor
!!!!!!!!!!!

Xcursor.theme: basewhite
! Xcursor.size:  22

! URXVT
!!!!!!!!

! Font
URxvt.font: xft:Inconsolata:pixelsize=15

! Clickable links
URxvt.perl-ext:       default,matcher
URxvt.urlLauncher:    /usr/local/bin/opera
URxvt.matcher.button: 1 

! Other options
URxvt.saveLines:  65534
URxvt.visualBell: true
URxvt.scrollBar:  false

! Colors (modified theme one from http://phraktured.net/terminal-colors/)
URxvt.background: rgb:00/00/00
